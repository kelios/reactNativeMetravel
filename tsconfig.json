{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react",
    "module": "commonjs",
    "lib": ["ES2015", "dom"],
    "paths": {
      "@/*": ["./*"]
    },
    "typeRoots": ["./src/types"]
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
